{
    "name": "ro0nl/http-responder",
    "type": "symfony-bundle",
    "description": "A Symfony HTTP responder",
    "keywords": [
        "symfony",
        "twig",
        "http",
        "responder",
        "adr"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roland Franssen",
            "email": "franssen.roland@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/http-foundation": "^3.4 || ^4.2"
    },
    "require-dev": {
        "symfony/routing": "^3.4 || ^4.2",
        "twig/twig": "^1.34 || ^2.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "ro0NL\\HttpResponder\\": "src"
        }
    },
    "minimum-stability": "dev"
}
